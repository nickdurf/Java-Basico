package JavaBasico;

public class Ejercicio1 {

    public static void main(String[] args) {

        // 1. numericos

        // 1.1 enteros
        byte variable1 = 5;
        short variable2 = 10;
        int variable3 = 30;
        long variable4 = 100;

        // 1.2 decimales
        float variable5 = 5.5f;
        double variable6 = 10.5d;

        // 2. booleano
        boolean variable7 = false;
        boolean variable8 = true;

        // 3. texto
        char variable9 = 'a';
        String variable10 = "Hola Mundo";

        System.out.println("Variable tipo byte = " + variable1);
        System.out.println("Variable tipo short = " + variable2);
        System.out.println("Variable tipo int = " + variable3);
        System.out.println("Variable tipo long = " + variable4);
        System.out.println("Variable tipo float = " + variable5);
        System.out.println("Variable tipo double = " + variable6);
        System.out.println("Variable tipo boolean = " + variable7);
        System.out.println("Variable tipo boolean = " + variable8);
        System.out.println("Variable tipo char = " + variable9);
        System.out.println("Variable tipo String = " + variable10);

    }
}
